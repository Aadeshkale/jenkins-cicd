pipeline{
    agent any

    stages{ 

        stage("System-ENVS") {
            steps{
                sh 'printenv | sort'
            }
        }

        stage('Build Image'){
            steps{
                script{
                    if (env.GIT_BRANCH == 'main'){
                        sh "echo This is main Branch"
                    }
                }
            }

        }


    }   
}